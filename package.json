{
  "name": "@aureooms/js-operator",
  "description": "language operators code bricks for JavaScript",
  "version": "1.0.2",
  "license": "AGPL-3.0",
  "author": "aureooms",
  "homepage": "https://make-github-pseudonymous-again.github.io/js-operator",
  "repository": {
    "url": "https://github.com/make-github-pseudonymous-again/js-operator",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/make-github-pseudonymous-again/js-operator/issues"
  },
  "keywords": [
    "bricks",
    "functional",
    "javascript",
    "js",
    "operator"
  ],
  "sideEffects": false,
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "NODE_ENV=production microbundle",
    "cover": "NODE_ENV=cover c8 --all --src src --reporter lcov --reporter text-summary --reporter text npm test",
    "dev": "npm run lint-config-and-fix && npm run lint-and-fix && npm run cover -- -- -st --fail-fast",
    "esdoc": null,
    "lint": "true",
    "lint-config": "fixpack --dryRun",
    "lint-config-and-fix": "fixpack || fixpack",
    "prepare": "npm run build",
    "release": "np --message ':hatching_chick: release: Bumping to v%s.'",
    "test": "ava",
    "travis": null
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "7.14.0",
    "@babel/preset-env": "7.14.0",
    "@babel/register": "7.13.16",
    "@commitlint/cli": "12.1.1",
    "@js-library/commitlint-config": "0.0.4",
    "ava": "3.15.0",
    "babel-plugin-transform-remove-console": "6.9.4",
    "babel-plugin-unassert": "3.0.1",
    "babel-preset-power-assert": "3.0.0",
    "c8": "7.7.1",
    "coveralls": "3.1.0",
    "esdoc": "1.1.0",
    "esdoc-inject-script-plugin": "1.0.0",
    "esdoc-inject-style-plugin": "1.0.0",
    "esdoc-standard-plugin": "1.0.0",
    "fixpack": "4.0.0",
    "husky": "6.0.0",
    "lint-staged": "10.5.4",
    "microbundle": "0.13.0",
    "np": "7.5.0",
    "pinst": "2.1.6",
    "power-assert": "1.6.1",
    "xo": "0.39.1"
  },
  "ava": {
    "files": [
      "test/src/**/*"
    ],
    "require": [
      "regenerator-runtime/runtime",
      "@babel/register"
    ],
    "timeout": "1m"
  },
  "babel": {
    "sourceMaps": true,
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": "current node"
        }
      ]
    ],
    "plugins": [
      [
        "transform-remove-console",
        {
          "exclude": [
            "log",
            "error",
            "warn"
          ]
        }
      ]
    ],
    "env": {
      "debug": {
        "presets": [
          "babel-preset-power-assert"
        ],
        "plugins": [
          [
            "transform-remove-console",
            {
              "exclude": [
                "debug",
                "log",
                "error",
                "warn"
              ]
            }
          ]
        ]
      },
      "test": {
        "presets": [
          "babel-preset-power-assert"
        ]
      },
      "cover": {
        "sourceMaps": "both",
        "presets": [
          "babel-preset-power-assert"
        ]
      },
      "development": {
        "presets": [
          [
            "@babel/preset-env",
            {
              "targets": [
                "defaults",
                "maintained node versions"
              ]
            }
          ],
          "babel-preset-power-assert"
        ]
      },
      "production": {
        "presets": [
          [
            "@babel/preset-env",
            {
              "targets": [
                "defaults",
                "maintained node versions"
              ]
            }
          ]
        ],
        "plugins": [
          "babel-plugin-unassert"
        ]
      }
    }
  }
}
